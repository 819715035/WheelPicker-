<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WheelYearView">
        <attr name="wv_startYear" format="integer" />
        <attr name="wv_endYear" format="integer" />
        <attr name="wv_selectedYear" format="integer" />
        <attr name="wv_maxSelectedYear" format="integer" />
        <attr name="wv_minSelectedYear" format="integer" />
    </declare-styleable>

    <declare-styleable name="WheelMonthView">
        <attr name="wv_selectedMonth" format="integer" />
        <attr name="wv_maxSelectedMonth" format="integer" />
        <attr name="wv_minSelectedMonth" format="integer" />
    </declare-styleable>

    <declare-styleable name="WheelDayView">
        <attr name="wv_year" format="integer" />
        <attr name="wv_month" format="integer" />
        <attr name="wv_selectedDay" format="integer" />
        <attr name="wv_minSelectedDay" format="integer" />
        <attr name="wv_maxSelectedDay" format="integer" />
    </declare-styleable>

    <declare-styleable name="WheelHourView">
        <attr name="wv_is24Hour" format="boolean" />
        <attr name="wv_selectedHour" format="integer" />
        <attr name="wv_minSelectedHour" format="integer" />
        <attr name="wv_maxSelectedHour" format="integer" />
    </declare-styleable>

    <declare-styleable name="WheelMinuteView">
        <attr name="wv_selectedMinute" format="integer" />
        <attr name="wv_minSelectedMinute" format="integer" />
        <attr name="wv_maxSelectedMinute" format="integer" />
    </declare-styleable>

    <declare-styleable name="WheelSecondView">
        <attr name="wv_selectedSecond" format="integer" />
        <attr name="wv_minSelectedSecond" format="integer" />
        <attr name="wv_maxSelectedSecond" format="integer" />
    </declare-styleable>
</resources>